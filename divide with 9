for i in range(1, 100):
    if i % 9 != 0:
        continue  # Skip numbers not divisible by 9
    elif i == 27:
        break     # Exit the loop when i becomes 27
    else:
        pass      # Explicitly do nothing (dummy placeholder)

    # This block runs only for numbers divisible by 9 before 27
    print(i)
    output:
    9
    18
